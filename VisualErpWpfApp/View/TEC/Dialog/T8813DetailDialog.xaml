                    <TextBlock VerticalAlignment="Center" Grid.Column="0" Grid.Row="13" Margin="0,0,0,0"  Text="라벨지(A4) : " HorizontalAlignment="Left" TextAlignment="Right" FontSize="20"  />
                    <dxe:SpinEdit Increment="1" MaxValue="100" MinValue="1" Height="40" x:Name="text_Label_Cnt" Grid.Column="1" Grid.ColumnSpan="2" Margin="5,0,5,0" HorizontalAlignment="Left" Text="1" VerticalAlignment="Stretch" Width="100" MaxLength="18" Grid.Row="13" MaskType="Numeric" Mask="##0" MaskUseAsDisplayFormat="True" FontSize="20"/>
                    <Button x:Name="LabelButton" Height="40" Width="60" Margin="0" Grid.Column="1" VerticalAlignment="Center"  HorizontalAlignment="Right" Grid.Row="13" ToolTip="라벨지(A4)">
                        <StackPanel HorizontalAlignment="Center" Height="Auto" Orientation="Horizontal">
                            <Image Width="30" Height="30" Source="{dx:DXImage Image=Print_32x32.png}" Stretch="Fill"/>
                        </StackPanel>
                    </Button>

                    <!--<TextBlock VerticalAlignment="Center" Grid.Column="0" Grid.Row="13" Margin="0,0,0,0"  Text="  ( " HorizontalAlignment="Left" TextAlignment="Right" FontSize="13"  />
                    <TextBlock VerticalAlignment="Center" Grid.Column="0" Grid.Row="13" Margin="0,0,0,0"  Text="{Binding ElementName=ViewGridMst, Path=SelectedItems.Count}" HorizontalAlignment="Left" TextAlignment="Right" FontSize="13"  FontWeight="Bold" />
                    <TextBlock VerticalAlignment="Center" Grid.Column="0" Grid.Row="13" Margin="0,0,0,0"  Text=" ) " HorizontalAlignment="Left" TextAlignment="Right" FontSize="13"  />-->
                </StackPanel>
            </GroupBox>



            <GroupBox Header="바코드 출력" Grid.Row="0" Grid.Column="2" Margin="0,5,0,0" FontSize="10" Width="338">
                <StackPanel Grid.Row="0" Grid.Column="3" Orientation="Horizontal" >
                    <Image Width="20" Height="20" Grid.Row="13" Stretch="Fill" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0" Source="{dx:DXImage SvgImages/Business Objects/BO_Validation.svg}"/>
                    <TextBlock VerticalAlignment="Center" Grid.Column="1" Grid.Row="13" Margin="0" Text="바코드(Box) : " HorizontalAlignment="Left" TextAlignment="Right" FontSize="20" />
                    <dxe:SpinEdit Increment="1" MaxValue="100" MinValue="1" Height="40" x:Name="text_Barcode_Cnt" Width="100" Grid.Column="0" HorizontalAlignment="Right" Text="1" VerticalAlignment="Stretch"  MaxLength="18" Grid.Row="13" MaskType="Numeric" Mask="##0" MaskUseAsDisplayFormat="True" FontSize="20" />
                    <Button x:Name="OKButton" Height="40" Width="55" Margin="10,0,50,0" Grid.Column="1" VerticalAlignment="Center"  HorizontalAlignment="Right" Grid.Row="13">
                        <StackPanel HorizontalAlignment="Left" Width="62" Height="Auto" Orientation="Horizontal">
                            <Image Width="30" Height="30" Source="{dx:DXImage Image=Barcode_32x32.png}" Stretch="Fill"/>
                        </StackPanel>
                    </Button>
                </StackPanel>
            </GroupBox>


            <StackPanel Grid.Row="0" Grid.Column="4" Orientation="Horizontal">
                <Button x:Name="CancelButton" Height="50" Margin="30,0,5,0" VerticalAlignment="Center" Grid.Column="1" HorizontalAlignment="Right" Width="120" Grid.Row="13" Click="CancelButton_Click" >
                    <StackPanel HorizontalAlignment="Left" Width="80" Height="Auto" Orientation="Horizontal">
                        <Image Width="30" Height="30" Source="{dx:DXImage Image=Cancel_32x32.png}" Stretch="Fill"/>
                        <TextBlock Margin="5,0,50,0" VerticalAlignment="Center" FontSize="20" Text="닫기" TextWrapping="Wrap" Width="70"/>
                    </StackPanel>
                </Button>
            </StackPanel>
        </Grid>


    </Grid>
</dx:DXWindow>
