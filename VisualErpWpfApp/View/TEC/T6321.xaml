                                            </dxg:GridColumn>

                                            <dxg:GridColumn FieldName="MTRL_MAKE_DT" Width="90"  Header="제조일자"  HorizontalHeaderContentAlignment="Center" AllowEditing="False" AllowResizing="True" FilterPopupMode="CheckedList"  >
