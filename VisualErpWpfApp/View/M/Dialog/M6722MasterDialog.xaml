                        <dxg:FormatCondition FieldName="RMN_QTY" Expression="Not IsNullOrEmpty([RMN_QTY])" PredefinedFormatName="YellowFillWithDarkYellowText"/>


                        <dxg:FormatCondition FieldName="ITM_PRC" Expression="Not IsNullOrEmpty([ITM_PRC])" PredefinedFormatName="BoldText"/>
