<UserControl x:Class="AquilaErpWpfApp3.View.TEC.T6321"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
