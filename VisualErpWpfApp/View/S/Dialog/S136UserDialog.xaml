                <TextBlock x:Name="text_GRP_NM" Height="20" VerticalAlignment="Center" Grid.Column="4" Grid.Row="2" Text="부서* : " Foreground="Blue" HorizontalAlignment="Right" TextAlignment="Right" Margin="0" FontWeight="Bold"/>
