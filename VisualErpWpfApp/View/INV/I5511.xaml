                <dxb:BarEditItem x:Name="M_FM_DT" Content="입고 일자 (From) : " EditValue="{Binding StartDt, Mode=TwoWay}">
                    <!--<dxb:BarEditItem.EditTemplate>
                        <DataTemplate>
                            <dxe:DateEdit Height="25" IsTextEditable="False" FontSize="14" ShowClearButton="False" Width="110" Text="{Binding DataContext.StartDt, RelativeSource={RelativeSource AncestorType=dxb:BarEditItemLinkControl}}" MaskType="DateTime" DisplayFormatString="yyyy-MM-dd" Mask="d" />
                        </DataTemplate>
                    </dxb:BarEditItem.EditTemplate>-->

                    <dxb:BarEditItem.EditSettings>
                        <dxe:DateEditSettings IsTextEditable="True" AllowDefaultButton="True" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" ShowWeekNumbers="True" />
                    </dxb:BarEditItem.EditSettings>
                </dxb:BarEditItem>

                <dxb:BarEditItem x:Name="M_TO_DT" Content="~ (To) : " EditValue="{Binding EndDt, Mode=TwoWay}">
                    <!--<dxb:BarEditItem.EditTemplate>
                        <DataTemplate>
                            <dxe:DateEdit Height="25" IsTextEditable="False" FontSize="14" ShowClearButton="False" Width="110" Text="{Binding DataContext.EndDt, RelativeSource={RelativeSource AncestorType=dxb:BarEditItemLinkControl}}" MaskType="DateTime" DisplayFormatString="yyyy-MM-dd" Mask="d" />
                        </DataTemplate>
                    </dxb:BarEditItem.EditTemplate>-->

                    <dxb:BarEditItem.EditSettings>
                        <dxe:DateEditSettings IsTextEditable="True" AllowDefaultButton="True" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" ShowWeekNumbers="True" />
                    </dxb:BarEditItem.EditSettings>
                </dxb:BarEditItem>

                <dxb:BarEditItem x:Name="M_AREA_NM" Content="사업장 : " EditValue="{Binding M_SL_AREA_NM, Mode=TwoWay}" >
