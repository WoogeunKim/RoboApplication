        <dxg:GridControl  Grid.Row="1" x:Name="ViewGridDialog" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="0,0,0,0" AutoExpandAllGroups="True"  SelectionMode="Row" >

