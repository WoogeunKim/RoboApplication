                                <dxb:BarButtonItemLink BarItemName="M_DTL_INSERT"/>
                                <dxb:BarButtonItemLink BarItemName="M_DTL_REFRESH"/>
                                <dxb:BarButtonItemLink BarItemName="M_DTL_UPDATE"/>
                                <dxb:BarButtonItemLink BarItemName="M_DTL_DELETE"/>
                            </dxr:RibbonPageGroup>-->
                            <dxr:RibbonPageGroup Caption="입고 자재 관리">
                                <dxb:BarButtonItemLink BarItemName="M_DTL_INSERT_IN"/>
                                <dxb:BarButtonItemLink BarItemName="M_DTL_INSERT_OTH"/>
                                <!--<dxb:BarButtonItemLink BarItemName="M_DTL_INSERT_PRO"/>-->
                                <dxb:BarButtonItemLink BarItemName="M_DTL_INSERT_OUT"/>
                            </dxr:RibbonPageGroup>
                            <dxr:RibbonPageGroup Caption="">
                                <dxb:BarButtonItemLink BarItemName="M_DTL_DELETE"/>
                            </dxr:RibbonPageGroup>
                            <!--<dxr:RibbonPageGroup Caption="재고 조정" x:Name="G_DTL_INSERT_INV">
                                <dxb:BarButtonItemLink BarItemName="M_DTL_INSERT_INV"/>
                            </dxr:RibbonPageGroup>-->
                            <dxr:RibbonPageGroup Caption="이월 조정">
                                <dxb:BarButtonItemLink BarItemName="M_MST_NEXT_MONTH"/>
                            </dxr:RibbonPageGroup>
                        </dxr:RibbonPage>
                    </dxr:RibbonDefaultPageCategory>
                </dxr:RibbonControl>

